{
  "mcpServers": {
    "deepwiki": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.deepwiki.com/sse"
      ]
    },
    "linear": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.linear.app/sse"
      ]
    },
    "github": {
      "command": "sh",
      "args": [
        "-c",
        "docker run -i --rm -e GITHUB_PERSONAL_ACCESS_TOKEN=$(gh auth token) ghcr.io/github/github-mcp-server"
      ]
    }
  }
}
